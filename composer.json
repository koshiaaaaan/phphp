{
  "name": "phphp/phphp",
  "description": "PHP html parser",
  "license": "MIT",
  "require": {
    "php": ">=7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.4",
    "hoa/bench": "~3.0"
  },
  "autoload": {
    "psr-4": {
      "Phphp\\": "src/Phphp"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Phphp\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": [
      "phpunit --colors=always --testdox"
    ]
  }
}
